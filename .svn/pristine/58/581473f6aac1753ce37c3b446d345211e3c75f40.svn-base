/* testing different panels below */
#jqeasypanel{
	font-family:Arial;
    color:#fff;
    position:fixed;
	left:0;
	width:100%;
	height:75px;
	background:#65a0bd url("../images/bg.png") repeat-x left top;
	padding:0px 0px 5px 10px;
	overflow:hidden;
	display:none;
	z-index:1;
}
#jqeasypanel.top{
	top:0;
	border-bottom:1px solid #666;
}
#jqeasypanel.bottom {
	bottom:0;
	border-top:1px solid #666;
}
ul.panelbuttons {
	float:left;
}
ul.panelbuttons li {
	float:left;
	display:inline;
	width:50px;
	height:50px;
	margin-right:10px;
	padding:5px;
	text-align:center;
	border:1px solid #666;
	border-radius: 5px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	background:transparent url("../images/buttun-bg.png") repeat-x top left;
}
    ul.panelbuttons li img {
    height:30px;
    width:30px;
    border:none;
    }
ul.panelbuttons li a {
	color:#FFF;
    font-size:10px;
    text-decoration:none;
	outline:none;
}
ul.panelbuttons li a:hover {
	color:#999999;
}
ul.panelbuttons li a img {
	display:block;
	margin:0 auto;
}
ul.panelbuttons li a:hover img {
	display:block;
	margin:0 auto;
    border:1px solid #fff;
}
#copy {
	float:left;
	width: 300px;
	font-size:10px;
}
#jqeasypaneloptions {
	font-family:Arial;
    color:#fff;
    float:right;
    padding-top:50px;
	margin:-10px 75px 0px 0;
}
#jqeasypaneloptions p {
	text-align:right;
}
#jqeasytrigger {
	position:fixed;
	right:5px;
	padding:5px 0px 0px 0px;
	width:160px;
	height:27px;
	background-color:none;
    background-image:url("../images/openToolsBG.png");
    background-repeat:repeat-x;
	text-align:center;
	font-size:14px;
	z-index:2;
    display:none;
}
#jqeasytrigger.top {
	top:0;
    border-bottom:1px solid #fff;
   border-right:1px solid #fff;
   border-left:1px solid #fff;
	border-bottom-right-radius: 5px;
	-moz-border-radius-bottomright: 5px;
	-webkit-border-bottom-right-radius: 5px;
	border-bottom-left-radius: 5px;
	-moz-border-radius-bottomleft: 5px;
	-webkit-border-bottom-left-radius: 5px;
}
#jqeasytrigger.bottom {
	bottom:0;
	border-top-right-radius: 5px;
	-moz-border-radius-topright: 5px;
	-webkit-border-top-right-radius: 5px;
	border-top-left-radius: 5px;
	-moz-border-radius-topleft: 5px;
	-webkit-border-top-left-radius: 5px;
}
#jqeasytrigger a {
	color:#FFF;
    
    text-decoration:none;
    
}
#jqeasytrigger a:hover {
   
	color:#CCC;
}
#jqeasytrigger a.open {
   font-family:'ArialBlackRegular', Arial, sans-serif;
   font-size:14pt;
   
	display:block;
}
#jqeasytrigger a.close {
   font-family:'ArialBlackRegular', Arial, sans-serif;
   
   font-size:14pt;
	display:none;	
}
ul.horizontal-dropdown {
    font-family: arial;
    padding: 0px 0px;
    margin:0;
    color: #fff;
    position: relative;
    float: left;
    list-style:none;
}
 
ul.horizontal-dropdown a {
    display: block;
    padding: 10px 40px;
    text-decoration: none;
    color: #fff;
    /*background: url(../images/border.gif) no-repeat right top;*/
}
 
ul.horizontal-dropdown li.last a {
    background: none;
}
 
ul.horizontal-dropdown li {
    float: left;
    padding: 0;
    background: url(../images/top-bg.gif) repeat-x;
    list-style-image:none;
    
}
 
ul.horizontal-dropdown li.hover {
    background-position: bottom;
}
 
/*sub nav*/
ul.horizontal-dropdown ul {
    position: absolute;
    left: 0;
    background: #3fb306;
    overflow: hidden;
    z-index:100;
}
 
ul.horizontal-dropdown ul li {
    background: transparent;
    padding: 0 0 0 0;
    float: left;
}
 
ul.horizontal-dropdown ul li a {
    position: relative;
    top: 0;
    left: 0;
    background: #3fb306;
    padding: 0;
    display: block;
    float: left;
    padding: 10px 20px;
}
 
ul.horizontal-dropdown ul li a:hover {
    color: #0609b3;
}
